  "scripts": {
    "start-local": "MADOC_KEY_PATH=../../var/certs/ JWT_REQUEST_DIR=./service-jwts JWT_RESPONSE_DIR=../../var/jwt node ./lib/index.js",
    "start": "NODE_ENV=development pm2 start ecosystem.config.js --watch --no-daemon",
    "storybook": "start-storybook -p 6500",
    "watch": "tsc -p --watch",
    "test": "jest",
    "build": "NODE_ENV=production yarn generate-schema && tsc -p . && yarn build:admin && yarn build:site",
    "generate-migration": "node migrate.js create",
    "generate-schema": "node generate-schemas.js",
    "build:admin": "MADOC_BUNDLE_ID=admin webpack --config webpack.frontend.js src/frontend/admin/client.tsx --output-path lib/frontend/admin/build",
    "build:site": "MADOC_BUNDLE_ID=site webpack --config webpack.frontend.js src/frontend/site/client.ts --output-path lib/frontend/site/build"
  },
